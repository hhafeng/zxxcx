/* pages/case/case.wxss */
page{
  height: 100%;
}
.casepage{
  width:100%;
  height: 100%;
  background: #eee;
}
.caselist {
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.caselist:after{
  content:'';
  width:47%;
}
.caseflex {
  width:47%;
  display: flex;
  margin: 10px 0px 0px 0px;
  box-sizing: border-box;
  border-radius: 8px 8px 0px 0px;
  background-color: #fff;
}
.casebox{
  width:100%;
}
.photobox{
  width:100%;
  height: 110px;
  overflow: hidden;
}
.casebox image{
  width:100%;
  border-radius: 8px 8px 0px 0px;
}
.casetitle{
  padding:0px 15px;
}