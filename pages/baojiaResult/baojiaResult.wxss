/* pages/baojiaResult/baojiaResult.wxss */
.baojia-result-box{
  display: flex;
  justify-content: center;
  margin: 30rpx 0;
}
.baojia-result{
  width: 90%;
  border-radius: 10rpx;
  border:solid 2rpx #eee;
}
.result-title{
  color:#000;
  padding:30rpx 0;
  text-align: center;
  background-color: #ccffff;
}
.result-title-t{
  font-size:32rpx;
}
.result-title-c{
  color:#f69c33;
}
.result-title-c text{
  font-size:50rpx;
  color:#f69c33;
}
.result-content{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding:30rpx;
  line-height: 180%;
  
}
.result-content view{
  color:#555;
  font-weight: bold;
}
.result-tip{
  width:90%;
  margin:0 auto 30rpx auto;
}
.result-tip-1{
  color:#f69c33;
}
.result-tip-2{
  color:#999;
}