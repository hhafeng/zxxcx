/**app.wxss**/
view,text {
  font-size:28rpx;
  color:#222;
}
.item-divider{
  height:24rpx;
  background-color: #efefef;
}
.hot-icon{
  background: url(data:image/png;base64,) no-repeat;
  width:24px;height:12px;background-size: contain;position: absolute;top: 1px;right: 0px;
}
.eye-icon{
  background: url(data:image/png;base64,) 0 center no-repeat;width:24px;height:12px;background-size: contain;
}
.star-icon{
  background: url(data:image/png;base64,) no-repeat;width:18px;height: 18px;background-size: contain;
}
.star-white-icon{
  background: url(data:image/png;base64,) no-repeat;width:18px;height: 18px;background-size: contain;
}
.item-right-icon{
  background: url(data:image/png;base64,) no-repeat;width:10px;height: 18px;background-size: contain;
}
.simple-title{
  display: flex;
  justify-content: space-between;
  padding:30rpx;
}
.simple-title-text{
  font-size:36rpx;
}
.simple-title-more{
  color:#888;
  font-size:24rpx;
}
.item-overflow-hidden{
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.lz-btn{
    display: inline-block;
    height: 45px;
    line-height: 45px;
    padding: 0 18px;
    white-space: nowrap;
    text-align: center;
    font-size: 18px;
    border: none;
    border-radius: 2px;
    cursor: pointer;
    outline: 0;
    box-sizing: border-box;
}
.lz-btn-block{
  width: 100%;
}
.lz-btn-default{
  background-color: #fc6a48;
  color:#fff;
}
.lz-btn-green{
  background-color: #34c083;
  color:#fff;
}
.m-p-r{
  padding-right: 16rpx;
}
.lz-item{
  display: flex;
  border-bottom: solid 2rpx #f1f1f1;
  align-items: center;
  padding:24rpx 20rpx;
  position: relative;
}
.lz-item .item-right{
  position: absolute;
  right:20rpx;
  top:calc(50%-20rpx);
}
.lz-item .item-content{
  padding-right:48rpx;
  padding-left:16rpx;
  flex:1;
}
.lz-item-title{
  padding:16rpx 30rpx;
  background-color: #f1f1f1;
  display: flex;
  align-items: center;
  font-size:32rpx;
  justify-content: space-between;
}
.bg-white{background-color: #fff;}
.margin-top{margin-top:24rpx;}